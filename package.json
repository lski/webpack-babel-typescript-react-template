{
	"name": "spa-template",
	"version": "1.0.0",
	"description": "SPA Build Template",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"analysis": "npm run build:dev --env analysis && npm run build --env analysis",
		"build": "npx webpack --mode=\"production\"",
		"build:dev": "npx webpack",
		"lint": "npx eslint ./",
